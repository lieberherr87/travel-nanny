<%= simple_form_for @booking do |f| %>
  <div class="form-inputs">
    <%= f.input :start_date, required: true, autofocus: true %>
    <%= f.input :end_date, required: true %>
    <%= f.input :full_price %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, "Submit booking" %>
  </div>
<% end %>
